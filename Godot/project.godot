; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RaveSpin"
run/main_scene="uid://dixosugdlls6v"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="uid://couo6fmbphjlc"

[autoload]

XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"

[dotnet]

project/assembly_name="Rave Spin"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
textures/default_filters/anisotropic_filtering_level=1

[shader_globals]

